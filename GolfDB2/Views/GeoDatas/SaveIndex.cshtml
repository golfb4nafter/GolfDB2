@model IEnumerable<GolfDB2.Models.GeoData>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.GeoSpatialDataId)*@
            @Html.DisplayName("Course Name")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.GeoObjectDescription)*@
            @Html.DisplayName("Description")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.GeoObjectType)*@
            @Html.DisplayName("GPS")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.HoleId)*@
            @Html.DisplayName("Type")
        </th>
        <th>
            @*@Html.DisplayName("Hole Number")*@
            @Html.DisplayName("Hole Number")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.CourseId)*@
            @Html.DisplayName("Order Number (back to front)")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @*@Html.DisplayFor(modelItem => item.CourseId)*@
            @Html.DisplayName(GolfDB2.Models.MiscLists.GetCourseNameById(item.CourseId, null))
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GeoObjectDescription)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.GeoSpatialDataId)*@
            @Html.DisplayName(GolfDB2.Models.MiscLists.GetGeoSpatialDataPointDescriptionById(item.GeoSpatialDataId,null))
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.GeoObjectType)*@
            @Html.DisplayName(GolfDB2.Models.MiscLists.GetObjectTypeById(item.GeoObjectType,null))
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.HoleId)*@
            @Html.DisplayName(GolfDB2.Models.MiscLists.GetHoleNumberByHoleId(item.HoleId,null).ToString())
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
